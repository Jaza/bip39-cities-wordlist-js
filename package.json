{
  "name": "bip39-cities-wordlist",
  "version": "0.1.0",
  "description": "JS version of the BIP39 cities wordlist.",
  "main": "dist/lib.js",
  "scripts": {
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "build": "npm run lint && npm run build-ts",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "18.x.x",
    "npm": ">=8.0.0"
  },
  "author": "Jeremy Epstein",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Jaza/bip39-cities-wordlist-js",
  "keywords": [
    "bip39",
    "cities",
    "wordlist"
  ],
  "repository": "github:Jaza/bip39-cities-wordlist-js",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/tests/**/*.+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
